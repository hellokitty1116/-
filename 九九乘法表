import java.util.*;
public class ninenine {
	
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("請輸入行數");
		int n=sc.nextInt();
		System.out.println("請輸入列數");
		int m=sc.nextInt();
		
		for(int i=1;i<=n;i++) {
			for(int j=1;j<=m;j++) {
				System.out.print(i+"*"+j+"="+(i*j)+"\t");
			}
			System.out.print("\n");
		}
	}
}
